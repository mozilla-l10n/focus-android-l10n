msgctxt "preference_category_web_content"
msgid "Web Content"
msgstr "Agbur Web"

#. Preference category: stealth settings for while switching apps
msgctxt "preference_category_switching_apps"
msgid "Switching Apps"
msgstr "Tunigin gar yisnasen"

