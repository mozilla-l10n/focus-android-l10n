msgctxt "download_dialog_action_cancel"
msgid "Cancel"
msgstr "Uq'atexik"

#. Option in download dialogue to start a download
msgctxt "download_dialog_action_download"
msgid "Download"
msgstr "Taq uqasaxik"

#. The page html title (i.e. the <title> tag content)
msgctxt "errorpage_title"
msgid "Problem loading page"
msgstr "K'axk'olil rech uya'ik uchuq'ab' uxaq wuj"

msgctxt "errorpage_refresh"
msgid "Try Again"
msgstr "Chab'ana' chi jumul"

msgctxt "error_connectionfailure_title"
msgid "Unable to connect"
msgstr "Man kkowinb'ex ta ut'iqik"

msgctxt "error_connectionfailure_message"
msgid ""
"<ul>\n"
"        <li>The site could be temporarily unavailable or too busy. Try "
"again in a few moments.</li>\n"
"        <li>If you are unable to load any pages, check your mobile "
"device’s data or Wi-Fi connection.</li>\n"
"      </ul>"
msgstr ""
"<ul>\n"
"        <li>Le wokk'olib'al web' wene man jamal ta uwach on sib'alaj "
"latz' uwach. Chab'ana' chi jumul pa jutz'it.</li>\n"
"        <li>We man katkowin taj kaya' uchuq'ab' apachike taq uxaq wuj, "
"chaq'atuj le ujuq'attzij taq awiqkemchakub'al on ut'iqonem Wi-Fi .</li>\n"
"      </ul>"

msgctxt "error_timeout_title"
msgid "The connection timed out"
msgstr "Le t'iqonem xq'ax uq'ijol"

msgctxt "error_hostLookup_title"
msgid "Server not found"
msgstr "Man kriqitaj ta le kempatanel"

msgctxt "error_hostLookup_message"
msgid ""
"<ul>\n"
"        <li>Check the address for typing errors such as\n"
"          <strong>ww</strong>.example.com instead of\n"
"          <strong>www</strong>.example.com</li>\n"
"        <li>If you are unable to load any pages, check your device’s data"
" or Wi-Fi connection.</li>\n"
"      </ul>"
msgstr ""
"<ul>\n"
"        <li>Chaq'atuj le kemriqonib'al ruk' utz'ib'axik taq k'axk'olil "
"pacha'\n"
"          <strong>ww</strong>.example.com chi uwach\n"
"          <strong>www</strong>.example.com</li>\n"
"        <li>We man katkowin taj kaya' uchuq'ab' apachike uxaq wuj, "
"chaq'atuj le juq'attzij rech awiqkemchakub'al on t'iqonem rech.</li>\n"
"      </ul>"

msgctxt "error_malformedURI_title"
msgid "The address isn’t valid"
msgstr "Le kemriqonib'al man utz taj"

msgctxt "error_malformedURI_message"
msgid ""
"<ul>\n"
"  <li>Web addresses are usually written like "
"<strong>http://www.example.com/</strong></li>\n"
"  <li>Make sure that you’re using forward slashes (i.e. "
"<strong>/</strong>).</li>\n"
"</ul>"
msgstr ""
"<ul>\n"
"  <li>Le taq kemriqonib'al rech taq Web' nima amaq'el tz'ib'atal pacha' "
"<strong>http://www.example.com/</strong></li>\n"
"  <li>Qas chawila' chi ktajin kakoj q'e'l jeqk'utb'al (i.e. "
"<strong>/</strong>).</li>\n"
"</ul>"

msgctxt "error_redirectLoop_title"
msgid "The page isn’t redirecting properly"
msgstr "Le uxaq wuj man utz ta ub'e'ik ktajin kub'ano"

msgctxt "error_redirectLoop_message"
msgid ""
"<ul>\n"
"  <li>This problem can sometimes be caused by disabling or refusing to "
"accept cookies.</li>\n"
"</ul>"
msgstr ""
"<ul>\n"
"  <li>We k'eyowal k'or are kya'ow le uchupik on le man kajawataj ta "
"uk'amik cookies.</li>\n"
"</ul>"

msgctxt "error_unsupportedprotocol_title"
msgid "The address wasn’t understood"
msgstr "Man xk'oxomax ta le kemriqonib'al"

msgctxt "error_unsupportedprotocol_message"
msgid ""
"<ul>\n"
"  <li>You might need to install other software to open this address.</li>"
"\n"
"</ul>"
msgstr ""
"<ul>\n"
"  <li>Wene rajawaxik kajeqeb'a' jun chi cholwach.</li>\n"
"</ul>"

msgctxt "error_sslhandshake_title"
msgid "Secure connection failed"
msgstr "Man utz ta xub'an le chajital t'iqonem"

msgctxt "error_sslhandshake_message"
msgid ""
"<ul>\n"
"  <li>This could be a problem with the server’s configuration, or it "
"could be\n"
"someone trying to impersonate the server.</li>\n"
"  <li>If you have connected to this server successfully in the past, the "
"error may\n"
"be temporary, and you can try again later.</li>\n"
"</ul>"
msgstr ""
"<ul>\n"
"  <li>Wa' wene kuya' k'eyowal ruk' le suk'manem rech kempatanel, on wene "
"are jun le kraj kukoj rib' chi kempatane.l</li>\n"
"  <li>We utz ub'anom le at'iqonem ruk' we kempatanel le ub'anom kanoq, le"
" k'axk'olil wene xa ch'ikital uq'ijol, chi'l utz kanb'an chi jumul pa "
"jutz'it.</li>\n"
"</ul>"

msgctxt "error_generic_title"
msgid "Oops"
msgstr "Amalay"

msgctxt "error_generic_message"
msgid "We can’t load this page for some reason."
msgstr "Jas na k'u che man kojkowin taj kqaya' uchuq'ab' we uxaq wuj."

