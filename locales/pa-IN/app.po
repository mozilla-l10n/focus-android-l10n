msgctxt "menu_whats_new"
msgid "What's New"
msgstr "ਨਵਾਂ ਕੀ ਹੈ"

msgctxt "menu_settings"
msgid "Settings"
msgstr "ਸੈਟਿੰਗਾਂ"

msgctxt "menu_about"
msgid "About"
msgstr "ਇਸ ਬਾਰੇ"

msgctxt "menu_help"
msgid "Help"
msgstr "ਮਦਦ"

