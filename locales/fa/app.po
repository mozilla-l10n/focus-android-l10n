msgctxt "preference_privacy_should_block_cookies_yes_option"
msgid "Yes"
msgstr "بله"

#. New four options for Cookie blocking
msgctxt "preference_privacy_block_cookies_no"
msgid "No (allow all cookies)"
msgstr ""

msgctxt "preference_privacy_block_cookies_third_party_tracker"
msgid "Block third-party trackers cookies (recommended)"
msgstr ""

msgctxt "preference_privacy_block_cookies_third_party_only"
msgid ""
"Block all third-party cookies \n"
"(may cause websites to break)"
